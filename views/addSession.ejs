<%- include('partials/_header.ejs') %>
<script>

    async function saveSessionDB() {
        await localStorage.setItem('sessionId', String("<%= sessionId %>"));
        await localStorage.setItem('userType', String("<%= userType %>"));
        await localStorage.setItem('userName', String("<%= userName %>"));
        await localStorage.setItem('userId', String("<%= userId %>"));
        location.href = '/classes/<%= unhashedIdType %>/<%= unhashedId %>'
    }

    saveSessionDB()
</script>
<%- include('partials/_footer.ejs') %>